@model string
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Kom-shop twój koszyk";
}
@if(TempData["message"] != null)
{
    <div class="alert alert-warning" style="margin: auto; width: 1175px;">
        <span class="fas fa-info-circle" style="margin-left: 10px; margin-top: 7px; margin-right: 10px;"></span><span style="margin-bottom: auto">@TempData["message"]</span>
    </div>
}

<div id="details">
    @Html.Action("Details")
</div>
<div class="text-left" style="margin-left: 40px; margin-top: 40px;">
    <a class="btn btn-secondary" href="@Model">Powrót</a>
</div>